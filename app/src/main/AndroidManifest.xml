<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.dogs">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_launcher_icon"
        android:supportsRtl="true">
        <activity
            android:name="com.example.android.dogs.LaunchScreenActivity"
            android:theme="@style/LaunchScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.dogs.MainActivity"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>